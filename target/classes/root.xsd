<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="root" type="rootType"/>
  <xs:complexType name="rootType">
    <xs:sequence>
      <xs:element type="AccountType" name="Account"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AccountType">
    <xs:sequence>
      <xs:element type="xs:string" name="CustomerName"/>
      <xs:element type="xs:string" name="BusinessEntityName"/>
      <xs:element type="xs:string" name="Worker_Category_Reference_Type"/>
      <xs:element type="xs:string" name="Worker_Category_Reference_Value"/>
      <xs:element type="xs:string" name="Leave_Category_Reference_Type"/>
      <xs:element type="xs:string" name="Leave_Category_Reference_Value"/>
      <xs:element type="xs:string" name="First_Day_of_Leave"/>
      <xs:element type="xs:string" name="EstimatedLastDayOfLeave"/>
    </xs:sequence>
  </xs:complexType>
</xs:schema>